FoamFile
{
    format      ascii;
    class       volVectorField;
    location    "0";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0.01);

boundaryField
{
    constraint
    {
        type            symmetry;
    }
    wall
    {
        type            movingWallVelocity;
        value           uniform (0 0 0);
    }
    inlet
    {
        type            fixedNormalInletOutletVelocity;
        fixTangentialInflow true;
        normalVelocity  
        {
            type            fixedValue;
            value           uniform (0 0 0.01);
        }
        value           uniform (0 0 0.01);
    }
    outlet
    {
        type            pressureInletOutletVelocity;
        value           uniform (0 0 0.01);
    }
    defaultFaces
    {
        type            slip;
    }
    procBoundary2to0
    {
        type            processor;
        value           uniform (0 0 0.01);
    }
    procBoundary2to3
    {
        type            processor;
        value           uniform (0 0 0.01);
    }
}


// ************************************************************************* //
