FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "0";
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 100.200400801603;

boundaryField
{
    constraint
    {
        type            symmetry;
    }
    wall
    {
        type            zeroGradient;
    }
    inlet
    {
        type            zeroGradient;
    }
    outlet
    {
        type            totalPressure;
        rho             rho;
        psi             none;
        gamma           1;
        p0              uniform 0;
        value           uniform 100.200400801603;
    }
    defaultFaces
    {
        type            zeroGradient;
    }
    procBoundary2to0
    {
        type            processor;
        value           uniform 100.200400801603;
    }
    procBoundary2to3
    {
        type            processor;
        value           uniform 100.200400801603;
    }
}


